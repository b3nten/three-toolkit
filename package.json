{
  "name": "@elysiatech/engine",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "mkdist --src=src --dist=dist --format=esm -d --ext=js"
  },
  "exports": {
    ".": {
      "import": "./dist/mod.js",
      "types": "./dist/mod.d.ts"
    },
    "./game_objects/*": {
      "import": "./dist/game_objects/*.js",
      "types": "./dist/game_objects/*.d.ts"
    },
    "./behaviors/*": {
      "import": "./dist/behaviors/*.js",
      "types": "./dist/behaviors/*.d.ts"
    },
    "./render_pipelines": {
      "import": "./dist/render_pipelines.js",
      "types": "./dist/render_pipelines.d.ts"
    }
  },
  "peerDependencies": {
    "three": "^0.165.0",
    "postprocessing": "^6.35.4",
    "three-stdlib": "^2.32.2"
  },
  "devDependencies": {
    "@types/node": "^20.13.0",
    "@types/three": "^0.165.0",
    "mkdist": "^1.5.4",
    "typescript": "^5.4.5"
  }
}
